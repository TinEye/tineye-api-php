{
  "name": "tineye/tineye-api",
  "type": "library",
  "description": "Library to reverse image search using the TinEye API",
  "keywords": [
    "TinEye API",
    "Search by Image",
    "Reverse Image Search",
    "Find Duplicates",
    "Computer Vision"
  ],
  "homepage": "https://tineye.com",
  "license": "MIT",
  "authors": [
    {
      "name": "TinEye Inc.",
      "email": "support@tineye.com",
      "homepage": "https://github.com/TinEye",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "guzzlehttp/guzzle": "^6.3 || ^7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "tineye\\api\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tineye\\api\\": "tests\\Test*"
    }
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml.dist",
    "check-style": "phpcs src tests",
    "fix-style": "phpcbf src tests"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.0.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
